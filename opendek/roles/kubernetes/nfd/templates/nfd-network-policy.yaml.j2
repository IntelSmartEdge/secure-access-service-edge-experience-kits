kind: NetworkPolicy
apiVersion: networking.k8s.io/v1
metadata:
  name: nfd-to-nfd
  namespace: {{ system_namespace }}
spec:
  podSelector:
    matchLabels:
      app.kubernetes.io/name: node-feature-discovery
  ingress:
  - from:
    - podSelector:
        matchLabels:
          app.kubernetes.io/name: node-feature-discovery
